@Article{bouchand2019b,
    author = "Weng, Wenle and Bouchand, Romain and Lucas, Erwan and Kippenberg, Tobias J.",
    title = "Polychromatic {Cherenkov} {Radiation} {Induced} {Group} {Velocity} {Symmetry} {Breaking} in {Counterpropagating} {Dissipative} {Kerr} {Solitons}",
    volume = "123",
    issn = "0031-9007",
    url = "https://link.aps.org/doi/10.1103/PhysRevLett.123.253902",
    doi = "10.1103/PhysRevLett.123.253902",
    abstract = "The process of soliton Cherenkov radiation (also known as dispersive wave emission) in microresonator frequency combs plays a critical role in generating broadband and coherent microcomb spectra. Here, we report the observation of symmetry breaking in the group velocity of counter-propagating dissipative Kerr solitons, induced by polychromatic soliton Cherenkov radiation. Results show that in the presence of higher-order dispersion, the temporal arrangement of a multi-soliton state affects its group velocity, an effect which originates from the interference between multiple radiative tails emitted by individual solitons. Experimentally, we bidirectionally pump a microresonator with laser fields of equal power and frequency, and observe lifting of the degeneracy between the repetition rates of the counter-propagating solitons. The observation of symmetry breaking despite symmetric pumping conditions not only shines new light on the impact of dispersive waves on dissipative Kerr soliton dynamics, but also introduces a novel approach to develop coherent dual-comb spectrometers based on microcombs.",
    number = "25",
    journal = "Physical Review Letters",
    month = "December",
    year = "2019",
    note = "Publisher: American Physical Society",
    pages = "253902",
    file = "Weng\_2019\_Polychromatic\_Cherenkov\_Radiation\_Induced\_Group.pdf:/home/erwan/Zotero/storage/NYKW8INX/Weng\_2019\_Polychromatic\_Cherenkov\_Radiation\_Induced\_Group.pdf:application/pdf;Weng\_2019\_Polychromatic\_Cherenkov\_Radiation\_Induced\_Group.pdf:/home/erwan/Zotero/storage/DDT8D249/Weng\_2019\_Polychromatic\_Cherenkov\_Radiation\_Induced\_Group.pdf:application/pdf"
